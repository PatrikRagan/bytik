<p id="notice"><%= notice %></p>

<h1>Listing Scrapsites</h1>

<table>
  <thead>
    <tr>
      <th>Nehnutelnosti</th>
      <th>Bazos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scrapsites.each do |scrapsite| %>
      <tr>
        <td><%= scrapsite.nehnutelnosti %></td>
        <td><%= scrapsite.bazos %></td>
        <td><%= link_to 'Show', scrapsite %></td>
        <td><%= link_to 'Edit', edit_scrapsite_path(scrapsite) %></td>
        <td><%= link_to 'Destroy', scrapsite, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scrapsite', new_scrapsite_path %>
