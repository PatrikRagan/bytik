<% if user_signed_in? %>
    <%#= render 'logged_content' %>
    <%= link_to 'Logged In [click to logout]', destroy_user_session_path, :method => :delete, :class => "btn btn-success" %>
<% else %>
    <%= render 'jumbotron' %>
    <%= render 'not_logged_content' %>
    <%= render 'page_descriptions' %>
    <%= link_to 'Login', new_user_session_path, :class => "btn btn-success" %>
<% end %>
